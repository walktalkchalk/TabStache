{
  "manifest_version": 2,

  "name": "TabStache",
  "description": "This extension stashes the current tab session",
  "version": "1.0",
  "icons": {
    "128": "icon.png"
  },

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "bookmarks",
   "tabs"
   ]
}
